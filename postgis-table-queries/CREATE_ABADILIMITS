CREATE TABLE ABADILIMITS(
    ID                  SERIAL  PRIMARY KEY,
    ABADILIMITNAME      TEXT,
    NOOFPROPERTIES      TEXT,
    MARKINGSTARTDATE    TEXT,
    MARKINGENDDATE      TEXT,
    VILLAGENAME         TEXT,
    VILLAGELGDCODE      TEXT,
    GRAMPANCHAYAT       TEXT,
    HOBLI               TEXT,
    TALUK               TEXT,
    DISTRICT            TEXT,
    CREATORINFO         TEXT,
    APPROVERINFO        TEXT,
    GEOM                GEOMETRY(POLYGON, 3857)
);
